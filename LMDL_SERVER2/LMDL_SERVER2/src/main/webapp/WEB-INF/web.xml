<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <display-name>LMDL Server</display-name>
    <servlet>
        <servlet-name>ComprobarQR</servlet-name>
        <servlet-class>servlets.ComprobarQR</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetSistemasCliente</servlet-name>
        <servlet-class>servlets.GetSistemasCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InicioSesion</servlet-name>
        <servlet-class>servlets.InicioSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUsuariosSistema</servlet-name>
        <servlet-class>servlets.GetUsuariosSistema</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetHabitacionesSistema</servlet-name>
        <servlet-class>servlets.GetHabitacionesSistema</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUltRegistrosEstadisticosHabitacion</servlet-name>
        <servlet-class>servlets.GetUltRegistrosEstadisticosHabitacion</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ComprobarQR</servlet-name>
        <url-pattern>/ComprobarQR</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetSistemasCliente</servlet-name>
        <url-pattern>/GetSistemasCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InicioSesion</servlet-name>
        <url-pattern>/InicioSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUsuariosSistema</servlet-name>
        <url-pattern>/GetUsuariosSistema</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetHabitacionesSistema</servlet-name>
        <url-pattern>/GetHabitacionesSistema</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUltRegistrosEstadisticosHabitacion</servlet-name>
        <url-pattern>/GetUltRegistrosEstadisticosHabitacion</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
</web-app>
